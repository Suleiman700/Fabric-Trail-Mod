{
	"schemaVersion": 1,
	"id": "trailmod",
	"version": "${version}",
	"name": "Trail Mod",
	"description": "Draw a trail behind you while walking, Useful for tracing your path.",
	"authors": [
		"Suleiman"
	],
	"contact": {
		"homepage": "http://brightpixel.work/",
		"sources": "https://github.com/Suleiman700"
	},
	"license": "CC0-1.0",
	"icon": "assets/trailmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.ExampleMod"
		],
		"client": [
			"com.example.ExampleModClient"
		],
		"modmenu": [
			"com.example.ModMenuApiImpl"
		]
	},
	"mixins": [
		"trailmod.mixins.json",
		{
			"config": "trailmod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.2",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}